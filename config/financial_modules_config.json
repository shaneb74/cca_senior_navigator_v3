{
  "version": "3.0.0",
  "metadata": {
    "product_key": "cost_planner_v2",
    "display_name": "Financial Assessment",
    "description": "Comprehensive financial planning modules for senior care costs",
    "requires_auth": false,
    "completion_required": ["income", "assets"]
  },
  "modules": [
    {
      "key": "income",
      "title": "Income Sources",
      "icon": "üí∞",
      "description": "Monthly income from all sources",
      "estimated_time": "2-3 min",
      "required": true,
      "sort_order": 1,
      "sections": [
        {
          "id": "social_security",
          "title": "Social Security Benefits",
          "icon": "üèõÔ∏è",
          "help_text": "Monthly Social Security retirement or disability benefits",
          "fields": [
            {
              "key": "ss_monthly",
              "label": "Social Security Monthly Benefit",
              "type": "currency",
              "required": false,
              "min": 0,
              "max": 5000,
              "step": 10,
              "default": 0,
              "help": "Enter the monthly Social Security benefit amount",
              "display_format": "$/month"
            }
          ]
        },
        {
          "id": "pension",
          "title": "Pension & Annuity Income",
          "icon": "üìä",
          "help_text": "Retirement pension and annuity payments",
          "fields": [
            {
              "key": "pension_monthly",
              "label": "Monthly Pension Amount",
              "type": "currency",
              "required": false,
              "min": 0,
              "max": 20000,
              "step": 100,
              "default": 0,
              "help": "Monthly pension or annuity income"
            }
          ]
        },
        {
          "id": "employment",
          "title": "Employment Income",
          "icon": "üíº",
          "help_text": "Income from current employment or self-employment",
          "fields": [
            {
              "key": "employment_status",
              "label": "Employment Status",
              "type": "select",
              "options": [
                {"value": "not_employed", "label": "Not employed"},
                {"value": "part_time", "label": "Part-time employment"},
                {"value": "full_time", "label": "Full-time employment"},
                {"value": "self_employed", "label": "Self-employed"}
              ],
              "default": "not_employed"
            },
            {
              "key": "employment_monthly",
              "label": "Monthly Employment Income (after tax)",
              "type": "currency",
              "required": false,
              "min": 0,
              "max": 50000,
              "step": 100,
              "default": 0,
              "help": "Net monthly income from employment",
              "visible_if": {
                "field": "employment_status",
                "not_equals": "not_employed"
              }
            }
          ]
        },
        {
          "id": "investment_income",
          "title": "Investment Income",
          "icon": "üìà",
          "help_text": "Dividends, interest, and rental income",
          "fields": [
            {
              "key": "investment_monthly",
              "label": "Monthly Investment Income",
              "description": "dividends, interest, rental",
              "type": "currency",
              "required": false,
              "min": 0,
              "max": 50000,
              "step": 100,
              "default": 0,
              "help": "Investment dividends, interest, rental property income"
            }
          ]
        },
        {
          "id": "other_income",
          "title": "Other Income Sources",
          "icon": "üíµ",
          "fields": [
            {
              "key": "other_monthly",
              "label": "Other Monthly Income",
              "description": "family support, trust, etc.",
              "type": "currency",
              "required": false,
              "min": 0,
              "max": 50000,
              "step": 100,
              "default": 0,
              "help": "Family support, trust distributions, other income"
            }
          ]
        }
      ],
      "summary": {
        "type": "calculated",
        "label": "Total Monthly Income",
        "formula": "sum(ss_monthly, pension_monthly, employment_monthly, investment_monthly, other_monthly)",
        "display_format": "${:,.0f}/month"
      },
      "output_contract": {
        "ss_monthly": "number",
        "pension_monthly": "number",
        "employment_status": "string",
        "employment_monthly": "number",
        "investment_monthly": "number",
        "other_monthly": "number",
        "total_monthly_income": "calculated"
      }
    },
    {
      "key": "assets",
      "title": "Assets & Resources",
      "icon": "üè¶",
      "description": "Available financial assets and resources",
      "estimated_time": "3-4 min",
      "required": true,
      "sort_order": 2,
      "sections": [
        {
          "id": "liquid_assets",
          "title": "Liquid Assets",
          "icon": "üíµ",
          "help_text": "Easily accessible cash and savings",
          "layout": "two_column",
          "fields": [
            {
              "key": "checking_savings",
              "label": "Checking & Savings Accounts",
              "type": "currency",
              "required": false,
              "min": 0,
              "max": 10000000,
              "step": 1000,
              "default": 0,
              "help": "Total in bank accounts",
              "column": 1
            },
            {
              "key": "cds_money_market",
              "label": "CDs & Money Market Accounts",
              "type": "currency",
              "required": false,
              "min": 0,
              "max": 10000000,
              "step": 1000,
              "default": 0,
