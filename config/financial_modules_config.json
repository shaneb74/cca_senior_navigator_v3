          ],
          "summary": {
            "type": "calculated",
            "label": "Total Liquid Assets",
            "formula": "sum(checking_savings, cds_money_market)",
            "display_format": "${:,.0f}"
          }
        },
        {
          "id": "retirement_accounts",
          "title": "Retirement Accounts",
          "icon": "üìä",
          "help_text": "401(k), IRA, and other retirement savings",
          "layout": "two_column",
          "fields": [
            {
              "key": "ira_traditional",
              "label": "Traditional IRA",
              "type": "currency",
              "required": false,
              "min": 0,
              "max": 10000000,
              "step": 5000,
              "default": 0,
              "help": "Traditional IRA balance",
              "column": 1
            },
            {
              "key": "ira_roth",
              "label": "Roth IRA",
              "type": "currency",
              "required": false,
              "min": 0,
              "max": 10000000,
              "step": 5000,
              "default": 0,
              "help": "Roth IRA balance",
              "column": 1
            },
            {
              "key": "k401_403b",
              "label": "401(k) / 403(b)",
              "type": "currency",
              "required": false,
              "min": 0,
              "max": 10000000,
              "step": 5000,
              "default": 0,
              "help": "Employer retirement plan balance",
              "column": 2
            },
            {
              "key": "other_retirement",
              "label": "Other Retirement Accounts",
              "type": "currency",
              "required": false,
              "min": 0,
              "max": 10000000,
              "step": 5000,
              "default": 0,
              "help": "Other retirement savings (SEP IRA, pension plans)",
              "column": 2
            }
          ],
          "summary": {
            "type": "calculated",
            "label": "Total Retirement Accounts",
            "formula": "sum(ira_traditional, ira_roth, k401_403b, other_retirement)",
            "display_format": "${:,.0f}"
          }
        },
        {
          "id": "investments",
          "title": "Investments",
          "icon": "üìà",
          "help_text": "Stocks, bonds, mutual funds, and other investments",
          "layout": "two_column",
          "fields": [
            {
              "key": "stocks_bonds",
              "label": "Stocks & Bonds",
              "type": "currency",
              "required": false,
              "min": 0,
              "max": 50000000,
              "step": 10000,
              "default": 0,
              "help": "Individual stocks and bonds",
              "column": 1
            },
            {
              "key": "mutual_funds",
              "label": "Mutual Funds & ETFs",
              "type": "currency",
              "required": false,
              "min": 0,
              "max": 50000000,
              "step": 10000,
              "default": 0,
              "help": "Mutual funds and exchange-traded funds",
              "column": 2
            }
          ],
          "summary": {
            "type": "calculated",
            "label": "Total Investments",
            "formula": "sum(stocks_bonds, mutual_funds)",
            "display_format": "${:,.0f}"
          }
        },
        {
          "id": "real_estate",
          "title": "Real Estate",
          "icon": "üè†",
          "help_text": "Property excluding primary residence",
          "fields": [
            {
              "key": "primary_residence_value",
              "label": "Primary Residence Value",
              "type": "currency",
              "required": false,
              "min": 0,
              "max": 50000000,
              "step": 25000,
              "default": 0,
              "help": "Estimated market value of your home",
              "info": "‚ÑπÔ∏è May be protected under Medicaid rules"
            },
            {
              "key": "primary_residence_mortgage",
              "label": "Remaining Mortgage Balance",
              "type": "currency",
              "required": false,
              "min": 0,
              "max": 10000000,
              "step": 10000,
              "default": 0,
              "help": "Outstanding mortgage on primary residence"
            },
            {
              "key": "investment_property",
              "label": "Investment Property Value",
              "type": "currency",
              "required": false,
              "min": 0,
              "max": 50000000,
              "step": 25000,
              "default": 0,
              "help": "Rental properties, vacation homes, land"
            }
          ],
          "calculations": [
            {
              "key": "primary_equity",
              "formula": "primary_residence_value - primary_residence_mortgage",
              "label": "Home Equity"
            }
          ]
        },
        {
          "id": "other_assets",
          "title": "Other Assets",
          "icon": "üíé",
          "fields": [
            {
              "key": "business_value",
              "label": "Business Ownership Value",
              "type": "currency",
              "required": false,
              "min": 0,
              "max": 50000000,
              "step": 25000,
              "default": 0,
              "help": "Value of business interests"
            },
            {
              "key": "other_assets_value",
              "label": "Other Assets",
              "description": "vehicles, collections, etc.",
              "type": "currency",
              "required": false,
              "min": 0,
              "max": 10000000,
              "step": 5000,
              "default": 0,
              "help": "Vehicles, jewelry, collections, other valuables"
            }
          ]
        }
      ],
      "summary": {
        "type": "calculated",
        "label": "Total Available Assets",
        "formula": "sum(checking_savings, cds_money_market, ira_traditional, ira_roth, k401_403b, other_retirement, stocks_bonds, mutual_funds, investment_property, business_value, other_assets_value)",
        "display_format": "${:,.0f}"
      },
      "output_contract": {
        "checking_savings": "number",
        "cds_money_market": "number",
        "total_liquid_assets": "calculated",
        "ira_traditional": "number",
        "ira_roth": "number",
        "k401_403b": "number",
        "other_retirement": "number",
        "total_retirement_accounts": "calculated",
        "stocks_bonds": "number",
        "mutual_funds": "number",
        "total_investments": "calculated",
        "primary_residence_value": "number",
        "primary_residence_mortgage": "number",
        "primary_equity": "calculated",
        "investment_property": "number",
        "business_value": "number",
        "other_assets_value": "number",
        "total_available_assets": "calculated"
      }
    },
    {
      "key": "va_benefits",
      "title": "VA Benefits",
      "icon": "üéñÔ∏è",
      "description": "VA Disability and Aid & Attendance benefits",
      "estimated_time": "3-5 min",
      "required": false,
      "sort_order": 3,
      "sections": [
        {
          "id": "veteran_status",
          "title": "Veteran Status",
          "icon": "üéñÔ∏è",
          "help_text": "Determine eligibility for VA benefits",
          "fields": [
            {
              "key": "is_veteran",
              "label": "Are you (or your spouse) a wartime veteran?",
              "type": "checkbox",
              "default": false,
              "help": "Veterans and surviving spouses may qualify for benefits"
            },
            {
              "key": "veteran_relationship",
              "label": "Your Relationship",
              "type": "select",
              "options": [
                {"value": "veteran_alone", "label": "I am the veteran"},
                {"value": "veteran_with_spouse", "label": "I am the veteran (married)"},
                {"value": "spouse_of_veteran", "label": "I am the spouse of a veteran"},
                {"value": "surviving_spouse", "label": "I am the surviving spouse"}
              ],
              "default": "veteran_alone",
              "visible_if": {
                "field": "is_veteran",
                "equals": true
              }
            },
            {
              "key": "service_era",
              "label": "Service Era",
              "type": "select",
              "options": [
                {"value": "wwii", "label": "World War II (1941-1945)"},
                {"value": "korea", "label": "Korean War (1950-1953)"},
                {"value": "vietnam", "label": "Vietnam War (1964-1975)"},
                {"value": "gulf_war", "label": "Gulf War (1990-1991)"},
                {"value": "afghanistan_iraq", "label": "Afghanistan/Iraq (2001-present)"},
                {"value": "peacetime", "label": "Peacetime service"},
                {"value": "not_sure", "label": "Not sure"}
              ],
              "default": "not_sure",
              "visible_if": {
                "field": "is_veteran",
                "equals": true
              }
            }
          ]
        },
        {
          "id": "va_disability",
          "title": "VA Disability Compensation",
          "icon": "üè•",
          "help_text": "Monthly disability compensation from VA",
          "visible_if": {
            "field": "is_veteran",
            "equals": true
          },
          "fields": [
            {
              "key": "has_va_disability",
              "label": "Do you receive VA Disability Compensation?",
              "type": "checkbox",
              "default": false
            },
            {
              "key": "va_disability_rating",
              "label": "Disability Rating Percentage",
              "type": "select",
              "options": [
                {"value": "10", "label": "10%"},
                {"value": "20", "label": "20%"},
                {"value": "30", "label": "30%"},
                {"value": "40", "label": "40%"},
                {"value": "50", "label": "50%"},
                {"value": "60", "label": "60%"},
                {"value": "70", "label": "70%"},
                {"value": "80", "label": "80%"},
                {"value": "90", "label": "90%"},
                {"value": "100", "label": "100%"}
              ],
              "default": "10",
              "visible_if": {
                "field": "has_va_disability",
                "equals": true
              }
            },
            {
              "key": "va_disability_monthly",
              "label": "Monthly VA Disability Payment",
              "type": "currency",
              "required": false,
              "min": 0,
              "max": 5000,
              "step": 10,
              "default": 0,
              "help": "Enter your monthly VA disability compensation",
              "visible_if": {
                "field": "has_va_disability",
                "equals": true
              }
            }
          ]
        },
        {
          "id": "aid_attendance",
          "title": "VA Aid & Attendance",
          "icon": "üèõÔ∏è",
          "help_text": "Enhanced pension for veterans needing care assistance",
          "visible_if": {
            "field": "is_veteran",
            "equals": true
          },
          "fields": [
            {
              "key": "receives_aid_attendance",
              "label": "Do you currently receive Aid & Attendance benefits?",
              "type": "checkbox",
              "default": false
            },
            {
              "key": "aid_attendance_monthly",
              "label": "Monthly Aid & Attendance Benefit",
              "type": "currency",
              "required": false,
              "min": 0,
              "max": 3000,
              "step": 10,
              "default": 0,
              "help": "Enter your current monthly A&A benefit",
              "visible_if": {
                "field": "receives_aid_attendance",
                "equals": true
              }
            },
            {
              "key": "eligible_aid_attendance",
              "label": "Interested in applying for Aid & Attendance?",
              "type": "checkbox",
              "default": false,
              "visible_if": {
                "field": "receives_aid_attendance",
                "equals": false
              }
            },
            {
              "key": "estimated_aid_attendance",
              "label": "Estimated A&A Benefit",
              "type": "calculated",
              "source": "va_benefit_lookup",
              "display_format": "Up to ${:,.0f}/mo",
              "visible_if": {
                "field": "eligible_aid_attendance",
                "equals": true
              }
            }
          ],
          "info_boxes": [
            {
              "type": "success",
              "visible_if": {
                "field": "eligible_aid_attendance",
                "equals": true
              },
              "content": "### ‚úÖ You may be eligible for VA Aid & Attendance!\n\n**2025 Maximum Monthly Benefits:**\n- Veteran alone: $2,379\n- Veteran with spouse: $2,829\n- Surviving spouse: $1,537\n\n**Requirements:**\n- Served during wartime period\n- Need help with activities of daily living\n- Meet income and asset limits\n\nüí° **Tip:** We can connect you with VA benefit specialists to help with the application process."
            }
          ]
        }
      ],
      "summary": {
        "type": "calculated",
        "label": "Total VA Benefits",
        "formula": "sum(va_disability_monthly, aid_attendance_monthly)",
        "display_format": "${:,.0f}/month"
      },
      "output_contract": {
        "is_veteran": "boolean",
        "veteran_relationship": "string",
        "service_era": "string",
        "has_va_disability": "boolean",
        "va_disability_rating": "string",
        "va_disability_monthly": "number",
        "receives_aid_attendance": "boolean",
        "aid_attendance_monthly": "number",
        "eligible_aid_attendance": "boolean",
        "estimated_aid_attendance": "calculated",
        "total_va_benefits": "calculated"
      }
    },
    {
      "key": "health_insurance",
      "title": "Health Insurance",
      "icon": "üè•",
      "description": "Medicare, Medicaid, and other health coverage",
      "estimated_time": "4-6 min",
      "required": false,
      "sort_order": 4,
      "sections": [
        {
          "id": "medicare",
          "title": "Medicare Coverage",
          "icon": "üè•",
          "help_text": "Federal health insurance for seniors 65+",
          "fields": [
            {
              "key": "has_medicare",
              "label": "Are you enrolled in Medicare?",
              "type": "checkbox",
              "default": false
            },
            {
              "key": "medicare_parts",
              "label": "Medicare Parts",
              "type": "multiselect",
              "options": [
                {"value": "part_a", "label": "Part A (Hospital Insurance)"},
                {"value": "part_b", "label": "Part B (Medical Insurance)"},
                {"value": "part_d", "label": "Part D (Prescription Drug)"}
              ],
              "default": [],
              "visible_if": {
                "field": "has_medicare",
                "equals": true
              }
            },
            {
              "key": "medicare_advantage",
              "label": "Do you have Medicare Advantage (Part C)?",
              "type": "checkbox",
              "default": false,
              "visible_if": {
                "field": "has_medicare",
                "equals": true
              }
            },
            {
              "key": "medicare_supplement",
              "label": "Do you have a Medigap (Supplement) plan?",
              "type": "checkbox",
              "default": false,
              "visible_if": {
                "field": "has_medicare",
                "equals": true
              }
            },
            {
              "key": "medicare_monthly_premium",
              "label": "Total Monthly Medicare Premiums",
              "description": "Parts B, D, Advantage, or Supplement",
              "type": "currency",
              "required": false,
              "min": 0,
              "max": 1000,
              "step": 10,
              "default": 0,
              "help": "Total monthly premium you pay for all Medicare coverage",
              "visible_if": {
                "field": "has_medicare",
                "equals": true
              }
            }
          ],
          "info_boxes": [
            {
              "type": "info",
              "visible_if": {
                "field": "has_medicare",
                "equals": true
              },
              "content": "‚ÑπÔ∏è **Important:** Medicare does **not** typically cover long-term care costs in assisted living or memory care facilities. It may cover short-term skilled nursing care following hospitalization."
            }
          ]
        },
        {
          "id": "medicaid",
          "title": "Medicaid Coverage",
          "icon": "üèõÔ∏è",
          "help_text": "State/federal health program for those with limited income",
          "fields": [
            {
              "key": "has_medicaid",
              "label": "Are you enrolled in Medicaid?",
              "type": "checkbox",
              "default": false
            },
            {
              "key": "medicaid_status",
              "label": "Medicaid Status",
              "type": "select",
              "options": [
                {"value": "enrolled", "label": "Currently enrolled"},
                {"value": "pending", "label": "Application pending"},
                {"value": "planning", "label": "Planning to apply"},
                {"value": "not_eligible", "label": "Not eligible"}
              ],
              "default": "enrolled",
              "visible_if": {
                "field": "has_medicaid",
                "equals": true
              }
            },
            {
              "key": "medicaid_covers_ltc",
              "label": "Does Medicaid cover your long-term care?",
              "type": "checkbox",
              "default": false,
              "visible_if": {
                "field": "medicaid_status",
                "equals": "enrolled"
              }
            },
            {
              "key": "medicaid_monthly_coverage",
              "label": "Monthly Medicaid Coverage Amount",
              "type": "currency",
              "required": false,
              "min": 0,
              "max": 10000,
              "step": 100,
              "default": 0,
              "help": "Estimated monthly value of Medicaid coverage",
              "visible_if": {
                "field": "medicaid_covers_ltc",
                "equals": true
              }
            }
          ],
          "info_boxes": [
            {
              "type": "success",
              "visible_if": {
                "field": "has_medicaid",
                "equals": false
              },
              "content": "üí° **Medicaid Planning:** If you're not currently on Medicaid but may need it in the future, we can connect you with specialists who help with Medicaid planning and asset protection strategies."
            }
          ]
        },
        {
          "id": "ltc_insurance",
          "title": "Long-Term Care Insurance",
          "icon": "üíº",
          "help_text": "Private insurance that covers long-term care costs",
          "fields": [
            {
              "key": "has_ltc_insurance",
              "label": "Do you have long-term care insurance?",
              "type": "checkbox",
              "default": false
            },
            {
              "key": "ltc_provider",
              "label": "Insurance Company",
              "type": "text",
              "max_length": 100,
              "placeholder": "e.g., Genworth, Mutual of Omaha",
              "visible_if": {
                "field": "has_ltc_insurance",
                "equals": true
              }
            },
            {
              "key": "ltc_daily_benefit",
              "label": "Daily Benefit Amount",
              "type": "currency",
              "required": false,
              "min": 0,
              "max": 1000,
              "step": 10,
              "default": 0,
              "help": "Daily benefit amount from your policy",
              "visible_if": {
                "field": "has_ltc_insurance",
                "equals": true
              }
            },
            {
              "key": "ltc_monthly_benefit",
              "label": "Monthly Benefit",
              "type": "calculated",
              "formula": "ltc_daily_benefit * 30",
              "display_format": "${:,.0f}",
              "visible_if": {
                "field": "has_ltc_insurance",
                "equals": true
              }
            },
            {
              "key": "ltc_elimination_period",
              "label": "Elimination Period (Waiting Period)",
